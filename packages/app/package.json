{
  "name": "chaoscards-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production craco build",
    "dev": "craco start",
    "fix": "xo --fix",
    "lint": "xo",
    "serve": "craco start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "xo": {
    "prettier": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "import/order": "off",
      "n/file-extension-in-import": "off",
      "no-unused-vars": "off",
      "node/file-extension-in-import": "off",
      "operator-linebreak": "off",
      "unicorn/prefer-node-protocol": "off"
    }
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.cjs"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.9.5",
    "@mantine/hooks": "^5.9.5",
    "@tabler/icons": "^1.114.0",
    "cross-env": "^7.0.2",
    "firebase": "^9.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@craco/craco": "6.4.3",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "babel-plugin-macros": "^3.1.0",
    "cssnano": "^5.1.14",
    "postcss": "^8.4.20",
    "process": "^0.11.10",
    "tailwindcss": "^3.2.4",
    "twin.macro": "^3.1.0",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
